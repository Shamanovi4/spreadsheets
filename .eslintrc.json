{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "no-debugger":"off",
		"no-tabs": ["error", {"allowIndentationTabs": true}]
  }
}
